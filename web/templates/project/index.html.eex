<header>
  <h1>Projects</h1>
</header>

<ul class="list-inline">
  <li><%= link "New Project", to: project_path(@conn, :new), class: "btn btn-success" %></li>
</ul>

<div id="projects">
  <%= for project <- @projects do %>
    <h2><%= link project.title, to: project_path(@conn, :show, project) %></h2>
    <p><%= project.description %></p>
  <% end %>
</div>
